{
  "name": "asterism",
  "version": "1.10.21",
  "description": "Extensible & dynamic dashboard platform",
  "author": "Xavier Gouley <gxapplications@gmail.com> (asterism.tk)",
  "contributors": [],
  "license": "CC-BY-NC-SA-4.0",
  "scripts": {
    "start:dist": "npm run dist:compact && cross-env NODE_ENV=production node example/dist.js -- --build",
    "start": "cross-env NODE_ENV=development nodemon --watch lib/server --watch lib/plugins example/hot-reload.js -- --build",
    "start:no-build": "cross-env NODE_ENV=development nodemon --watch lib/server --watch lib/plugins example/hot-reload.js",
    "dist": "rimraf dist && babel lib --compact false --retain-lines --out-dir dist",
    "dist:compact": "rimraf dist && babel lib --no-comments --compact true --out-dir dist",
    "gen:cert": "./generate-certificates.sh",
    "gen:webPush": "./generate-webPush-keys.sh",
    "lint": "standard --verbose | snazzy",
    "test": "npm run dist && npm run test:rerun",
    "release": "npm run release:patch",
    "release:patch": "npm version patch -m \"Release new bugfix version %s\" && npm publish",
    "release:minor": "npm version minor -m \"Release new minor version %s\" && npm publish",
    "release:major": "npm version major -m \"Release new major version %s\" && npm publish",
    "test:rerun": "cross-env NODE_ENV=test mocha --require ignore-styles test",
    "dl-icons": "material-icons-downloader assets/test",
    "prepublishOnly": "npm run dist:compact",
    "preversion": "npm run lint && npm run test",
    "version": "npm run dist:compact",
    "postversion": "git push && git push --tags",
    "validate": "npm ls"
  },
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/gxapplications/asterism"
  },
  "bugs": {
    "url": "https://github.com/gxapplications/asterism/issues"
  },
  "homepage": "https://github.com/gxapplications/asterism",
  "keywords": [
    "gridifier",
    "react",
    "material",
    "asterism",
    "domotics"
  ],
  "engines": {
    "node": ">= 12.12.0",
    "npm": ">= 6.11.3"
  },
  "standard": {
    "env": {
      "node": true
    },
    "parser": "babel-eslint",
    "ignore": [
      "/docs/"
    ]
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "@babel/preset-env": "7.11.x",
    "@babel/preset-react": "7.10.x",
    "@babel/register": "7.11.x",
    "ajv": "6.12.x",
    "all-the-cities": "3.x",
    "annyang": "2.6.x",
    "asterism-plugin-library": "^2.4.6",
    "babel-loader": "8.1.x",
    "babel-plugin-css-modules-transform": "1.6.x",
    "body-parser": "1.x",
    "camelopard": "0.7.x",
    "classnames": "2.2.x",
    "colors": "1.4.x",
    "cookie-parser": "1.4.x",
    "cron": "1.8.x",
    "css-loader": "4.2.x",
    "debounce": "1.x",
    "expose-loader": "1.0.x",
    "express": "4.17.x",
    "express-ipfilter": "1.1.x",
    "felicity": "6.0.x",
    "file-loader": "6.1.x",
    "html5sortable": "0.9.x",
    "imports-loader": "1.1.x",
    "joi": "17.4.x",
    "jquery": "3.5.x",
    "materialize-css": "1.0.0",
    "mini-css-extract-plugin": "0.11.x",
    "moment": "2.29.x",
    "moment-timer": "1.3.x",
    "node-forge": "0.10.x",
    "node-sass": "4.14.x",
    "nosql": "6.1.x",
    "object-path": "0.11.x",
    "prop-types": "15.6.x",
    "randexp": "^0.5.3",
    "react": "16.2.x",
    "react-color": "2.13.x",
    "react-gridifier": "^1.4.0",
    "react-hot-loader": "3.1.x",
    "react-materialize": "3.3.3",
    "react-transition-group": "1.2.x",
    "sass-loader": "6.0.x",
    "sha1": "1.x",
    "socket.io": "2.0.x",
    "style-loader": "1.2.x",
    "suncalc": "1.8.x",
    "tsort": "0.0.1",
    "uuid": "3.x",
    "validate-npm-package-name": "3.x",
    "web-push": "3.4.x",
    "webpack": "4.x",
    "webpack-dev-middleware": "3.7.x",
    "webpack-hot-middleware": "2.25.x"
  },
  "devDependencies": {
    "acorn": "7.x",
    "@babel/cli": "7.11.x",
    "@babel/core": "7.11.x",
    "babel-eslint": "10.1.x",
    "chai": "4.x",
    "cross-env": "7.x",
    "enzyme": "3.x",
    "ignore-styles": "5.x",
    "jsdom": "16.x",
    "material-icons-downloader": "^1.3.0",
    "mocha": "6.x",
    "nodemon": "2.x",
    "precommit-hook": "3.0.0",
    "react-dom": "16.2.x",
    "rimraf": "3.x",
    "snazzy": "8.x",
    "standard": "14.x",
    "webpack-dev-server": "3.11.x"
  },
  "directories": {
    "lib": "./lib",
    "dist": "./dist"
  }
}
