{
  "name": "asterism",
  "version": "1.2.10",
  "description": "Extensible & dynamic dashboard platform",
  "author": "Xavier Gouley <gxapplications@gmail.com> (asterism.tk)",
  "contributors": [],
  "license": "CC-BY-NC-SA-4.0",
  "scripts": {
    "start:dist": "npm run dist:compact && cross-env NODE_ENV=production node example/dist.js -- --build",
    "start": "cross-env NODE_ENV=development nodemon --watch lib/server --watch lib/plugins example/hot-reload.js -- --build --toto='basta'",
    "start:debug": "cross-env NODE_ENV=development DEBUG=* nodemon --watch lib/server example/hot-reload.js -- --build",
    "start:no-build": "cross-env NODE_ENV=development nodemon --watch lib/server --watch lib/plugins example/hot-reload.js",
    "dist": "rimraf dist && babel lib --comments=true --compact=false --retainLines=true --out-dir dist",
    "dist:compact": "rimraf dist && babel lib --comments=false --compact=true --out-dir dist",
    "prepublishOnly": "npm run dist:compact",
    "gen:cert": "./generate-certificates.sh",
    "gen:webPush": "./generate-webPush-keys.sh",
    "lint": "standard --verbose | snazzy",
    "test": "npm run dist && cross-env NODE_ENV=test mocha --require ignore-styles",
    "test:rerun": "cross-env NODE_ENV=test mocha --require ignore-styles",
    "validate": "npm ls",
    "dl-icons": "material-icons-downloader assets/test"
  },
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/gxapplications/asterism"
  },
  "bugs": {
    "url": "https://github.com/gxapplications/asterism/issues"
  },
  "homepage": "https://github.com/gxapplications/asterism",
  "keywords": [
    "gridifier",
    "react",
    "material",
    "asterism",
    "domotics"
  ],
  "engines": {
    "node": "8.13.0 - 8.17.0",
    "npm": ">= 6.4.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "chrome": "45"
          }
        }
      ],
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ],
    "env": {
      "development": {
        "plugins": [
          [
            "css-modules-transform",
            {
              "preprocessCss": "./babel-sass-preprocess.js",
              "generateScopedName": "[local]",
              "extractCss": {
                "dir": "./dist/styles/",
                "relativeRoot": "./lib/",
                "filename": "[name].css"
              },
              "extensions": [
                ".css",
                ".sass",
                ".scss"
              ]
            }
          ]
        ]
      }
    }
  },
  "standard": {
    "env": {
      "node": true
    },
    "parser": "babel-eslint",
    "ignore": [
      "/docs/"
    ]
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "ajv": "^6.9.1",
    "all-the-cities": "2.0.x",
    "annyang": "^2.6.0",
    "asterism-plugin-library": "^2.3.0",
    "babel-loader": "7.1.4",
    "babel-plugin-css-modules-transform": "1.5.0",
    "babel-plugin-transform-object-rest-spread": "6.23.x",
    "babel-preset-env": "1.6.x",
    "babel-preset-react": "6.24.x",
    "babili-webpack-plugin": "0.1.x",
    "body-parser": "1.x",
    "camelopard": "0.7.x",
    "classnames": "2.2.x",
    "colors": "1.1.x",
    "cookie-parser": "1.4.x",
    "cron": "^1.7.2",
    "css-loader": "0.28.x",
    "debounce": "1.x",
    "expose-loader": "0.7.x",
    "express": "4.16.x",
    "express-ipfilter": "0.3.x",
    "extract-text-webpack-plugin": "3.x",
    "felicity": "2.4.x",
    "file-loader": "0.11.x",
    "html5sortable": "0.9.x",
    "imports-loader": "0.7.x",
    "joi": "13.x",
    "joi-browser": "13.x",
    "jquery": "3.x",
    "materialize-css": "1.0.0",
    "moment": "^2.22.0",
    "moment-timer": "^1.2.3",
    "node-forge": "0.8.x",
    "node-sass": "4.13.x",
    "nosql": "6.1.x",
    "object-path": "0.11.x",
    "prop-types": "15.6.x",
    "react": "16.2.x",
    "react-color": "2.13.x",
    "react-gridifier": "^1.1.0",
    "react-hot-loader": "3.1.x",
    "react-materialize": "3.3.3",
    "react-transition-group": "1.2.x",
    "sass-loader": "6.0.x",
    "sha1": "1.x",
    "socket.io": "2.0.x",
    "style-loader": "0.18.x",
    "suncalc": "^1.8.0",
    "tsort": "0.0.1",
    "uuid": "3.x",
    "validate-npm-package-name": "3.x",
    "web-push": "^3.4.3",
    "webpack": "3.x",
    "webpack-dev-middleware": "1.12.x",
    "webpack-hot-middleware": "2.x"
  },
  "devDependencies": {
    "babel-cli": "6.26.x",
    "babel-core": "6.26.x",
    "babel-eslint": "6.1.x",
    "chai": "4.x",
    "cross-env": "^5.x",
    "enzyme": "3.x",
    "ignore-styles": "5.x",
    "jsdom": "11.x",
    "material-icons-downloader": "^1.0.0",
    "mocha": "3.x",
    "nodemon": "1.x",
    "precommit-hook": "3.0.0",
    "react-dom": "16.2.x",
    "rimraf": "2.x",
    "snazzy": "7.x",
    "standard": "10.x",
    "webpack-dev-server": "2.x"
  },
  "directories": {
    "lib": "./lib",
    "dist": "./dist"
  }
}
